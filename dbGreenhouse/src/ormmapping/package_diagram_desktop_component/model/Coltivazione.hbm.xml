<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="package_diagram_desktop_component.model.Coltivazione" table="Coltivazione" lazy="false">
		<id name="attribute" column="Attribute" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="descrizione" cascade="lock" column="Descrizione_ColtivazioneAttribute" class="package_diagram_desktop_component.model.Descrizione_Coltivazione" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="ID" column="ID" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="stato" column="Stato" type="string" length="255" not-null="false" lazy="false"/>
		<property name="data_prossima_operazione" column="Data_prossima_operazione" type="date" not-null="false" lazy="false"/>
		<one-to-one name="area" class="package_diagram_desktop_component.model.Area_Coltivata" cascade="save-update,lock" property-ref="coltivazione" lazy="proxy" access="field"/>
		<set name="ORM_Impiegato" table="Coltivazione_Impiegato" lazy="true" cascade="save-update,lock">
			<key column="ColtivazioneAttribute" not-null="true"/>
			<many-to-many column="ImpiegatoAttribute" class="package_diagram_desktop_component.model.Impiegato">
			</many-to-many>
		</set>
	</class>
</hibernate-mapping>
