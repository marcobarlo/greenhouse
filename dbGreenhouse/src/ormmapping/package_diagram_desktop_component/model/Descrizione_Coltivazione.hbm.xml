<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="package_diagram_desktop_component.model.Descrizione_Coltivazione" table="Descrizione_Coltivazione" lazy="false">
		<id name="attribute" column="Attribute" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="ID" column="ID" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="tipo" column="Tipo" type="string" length="255" not-null="false" lazy="false"/>
		<property name="descrizione" column="Descrizione" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_Coltivazioni" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="Descrizione_ColtivazioneAttribute" not-null="true"/>
			<one-to-many class="package_diagram_desktop_component.model.Coltivazione"/>
		</set>
	</class>
</hibernate-mapping>
