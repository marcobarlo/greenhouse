<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="package_diagram_desktop_component.model.Sezione" table="Sezione" lazy="false">
		<id name="attribute" column="Attribute" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="ID" column="ID" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="identificativo" column="Identificativo" type="integer" length="10" not-null="true" lazy="false"/>
		<set name="ORM_Aree" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="SezioneAttribute" not-null="true"/>
			<one-to-many class="package_diagram_desktop_component.model.Area_Coltivata"/>
		</set>
	</class>
</hibernate-mapping>
